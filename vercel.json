{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/vercel_app_lightweight.py/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel_app_lightweight.py"
    }
  ],
  "functions": {
    "vercel_app_lightweight.py": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "public": true
} 